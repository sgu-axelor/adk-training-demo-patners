<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<menuitem title="Partners" name="menu-partners"
		icon-background="green" />

	<menuitem name="menu-partner-all" parent="menu-partners"
		title="All Partner" action="partner.all" tag-style="info"
		tag="information" />
	<action-view name="partner.all" title="All Partners"
		model="com.axelor.partner.db.Partner">
		<view type="cards" name="partner-card" />
		<view type="form" name="partner-form" />
		<view type="grid" name="partner-grid" />
		<view type="kanban" name="partner-kanban" />
		<context name="json-enhance" expr="true" />
	</action-view>

	<menuitem name="menu-partner-all-tree" parent="menu-partners"
		title="All Partner(tree)" action="partner-tree.all"
		tag-style="default" tag="Tree" />
	<action-view name="partner-tree.all"
		title="All Partners(tree)" model="com.axelor.partner.db.Partner">
		<view type="tree" name="partner-tree" />
	</action-view>

	<menuitem name="menu-catagory-all" parent="menu-partners"
		title="All Catagory" action="catagory.all" tag-style="warning"
		tag="warning" />
	<action-view name="catagory.all" title="All Catagory"
		model="com.axelor.partner.db.Catagory">
		<view type="grid" name="catagory-grid" />
	</action-view>

	<menuitem name="menu-company-all" parent="menu-partners"
		title="All Company" action="company.all" tag-style="success"
		tag="Alert" />
	<action-view name="company.all" title="All Company"
		model="com.axelor.partner.db.Company">
		<view type="grid" name="company-grid" />
		<view type="form" name="company-form" />
	</action-view>

	<menuitem name="menu-one-all" parent="menu-partners"
		title="Time period" action="one.all" icon-background="purple" />
	<action-view name="one.all" title="Time Periods"
		model="com.axelor.partner.db.One">
		<view type="grid" name="one-grid" />
		<view type="form" name="one-form" />
	</action-view>

	<menuitem name="menu-two-all" parent="menu-partners"
		title="Minimum Periods" action="two.all" icon-background="white" />
	<action-view name="two.all" title="Minimum Period"
		model="com.axelor.partner.db.Two">
		<view type="grid" name="two-grid" />
		<view type="form" name="two-form" />
	</action-view>

	<menuitem title="Orders" name="menu-orders"
		icon-background="red" />
	
	<menuitem title="All Orders" name="menu-all-order" action="order.all" parent="menu-orders"/>
	<action-view name="order.all" title="Orders" model="com.axelor.partner.db.OrderLine">
		<view type="grid" name="orderline-grid"/>
		<view type="form" name="orderline-form"/>
	</action-view>

</object-views>
  