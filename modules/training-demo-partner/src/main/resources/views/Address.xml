	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<form model="com.axelor.partner.db.Address" title="Address"
		name="address-form">
		<field name="street" />
		<field name="area" />
		<field name="city" />
		<field name="state" />
		<field name="zip" />
	</form>
	<grid model="com.axelor.partner.db.Address" title="Address"
		name="address-grid">
		<field name="street" />
		<field name="area" />
		<field name="city" />
		<field name="state" />
		<field name="zip" />
	</grid>

</object-views>